{
  "name": "evid-dgc",
  "version": "2.0.0",
  "description": "Secure blockchain evidence management system with admin controls",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "frontend": "cd public && python -m http.server 8080",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.html",
    "lint:fix": "eslint . --ext .js,.html --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "build": "echo 'Build completed successfully'",
    "security:audit": "npm audit",
    "validate:html": "html-validate public/*.html"
  },
  "keywords": ["blockchain", "evidence", "admin", "security", "supabase"],
  "author": "EVID-DGC Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "@supabase/supabase-js": "^2.39.0",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "html-validate": "^8.7.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}